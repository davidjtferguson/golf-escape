pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

function _init()

 cir={
  x=64,
  y=64,
  r=5,
  c=7,
  grow=true,
  speed=0.5
 }

end

function _update60()

 if cir.grow then
  cir.r+=cir.speed
 else
  cir.r-=cir.speed
 end
 
 if cir.r>60 then
  cir.grow=false
 elseif cir.r<1 then
  cir.grow=true
 end

end

function _draw()
 cls()

 --circfill(cir.x,cir.y,cir.r,cir.c)
 
-- local x=cir.x+cir.r*cos(angle)
-- local y=cir.y+cir.r*sin(angleinradians)

 for i=0.5,0.75,0.05 do
  local x=cir.x+cir.r*cos(i)
  local y=cir.y+cir.r*sin(i)

  pset(x,y,7)
  
  local xbit=x-cir.x
  local ybit=y-cir.y
  
  pset(cir.x-xbit,y,8)
    
  clip(cir.x-xbit,cir.y,x-xbit,y-ybit)

  map(0,0,0,0,16,16)
  
  clip()
 end
 
end
